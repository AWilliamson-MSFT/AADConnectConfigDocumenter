<synchronizationRule>
  <id>{d79c6b03-a3d5-4160-8b12-87eff97ab7be}</id>
  <internal-id>{d79c6b03-a3d5-4160-8b12-87eff97ab7be}</internal-id>
  <version>1</version>
  <name>In from AD - Computer Join</name>
  <description />
  <immutable-tag>Microsoft.InfromADComputerJoin.002</immutable-tag>
  <direction>Inbound</direction>
  <disabled>False</disabled>
  <connector>{ae4db4cc-b16b-4eb2-acf5-837ca84e4d7d}</connector>
  <sourceObjectType>computer</sourceObjectType>
  <targetObjectType>device</targetObjectType>
  <linkType>Provision</linkType>
  <precedence>159</precedence>
  <precedence-after>{00000000-0000-0000-0000-000000000000}</precedence-after>
  <precedence-before>{00000000-0000-0000-0000-000000000000}</precedence-before>
  <softDeleteExpiryInterval>00:00:00</softDeleteExpiryInterval>
  <EnablePasswordSync>false</EnablePasswordSync>
  <attribute-mappings>
    <mapping>
      <dest>accountEnabled</dest>
      <src>
        <attr>userAccountControl</attr>
      </src>
      <expression>IIF(BitAnd([userAccountControl],2)=0,True,False)</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>deviceId</dest>
      <src>
        <attr>objectGUID</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>sourceAnchor</dest>
      <src>
        <attr>objectGUID</attr>
      </src>
      <expression>ConvertToBase64([objectGUID])</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>displayName</dest>
      <src>
        <attr>displayName</attr>
        <attr>cn</attr>
      </src>
      <expression>IIF(IsNullOrEmpty([displayName]),[cn],[displayName])</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>objectSid</dest>
      <src>
        <attr>objectSid</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>sourceObjectType</dest>
      <src>Computer</src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>deviceOSVersion</dest>
      <src>
        <attr>operatingSystemVersion</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>deviceOSType</dest>
      <src>
        <attr>operatingSystem</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>deviceTrustType</dest>
      <src>ServerAd</src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping execute-once="true">
      <dest>cloudCreated</dest>
      <src>False</src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>userCertificate</dest>
      <src>
        <attr>userCertificate</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping execute-once="true">
      <dest>registeredOwnerReference</dest>
      <src>
        <attr>mS-DS-CreatorSID</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>cloudFiltered</dest>
      <src>
        <attr>userCertificate</attr>
      </src>
      <expression>IIF(IsNullOrEmpty([userCertificate]),True,NULL)</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
  </attribute-mappings>
  <relationshipCriteria>
    <conditions>
      <condition>
        <csAttribute>objectGUID</csAttribute>
        <ilmAttribute>deviceId</ilmAttribute>
      </condition>
    </conditions>
  </relationshipCriteria>
  <synchronizationCriteria />
</synchronizationRule>