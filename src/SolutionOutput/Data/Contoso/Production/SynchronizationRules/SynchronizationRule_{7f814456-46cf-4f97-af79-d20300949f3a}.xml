      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>memberCount</dest>
      <src>
        <attr>member</attr>
      </src>
      <expression>ValueCount("member")</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>objectSid</dest>
      <src>
        <attr>objectSid</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>objectSidString</dest>
      <src>
        <attr>objectSid</attr>
      </src>
      <expression>StringFromSid([objectSid])</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>proxyAddresses</dest>
      <src>
        <attr>proxyAddresses</attr>
      </src>
      <expression>RemoveDuplicates(Trim(ImportedValue("proxyAddresses")))</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>securityEnabled</dest>
      <src>
        <attr>groupType</attr>
      </src>
      <expression>CBool(BitAnd([groupType],-2147483648))</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>sourceAnchor</dest>
      <src>
        <attr>objectGUID</attr>
      </src>
      <expression>ConvertToBase64([objectGUID])</expression>
      <valueMergeType>Update</valueMergeType>
    </mapping>
    <mapping>
      <dest>sourceAnchorBinary</dest>
      <src>
        <attr>objectGUID</attr>
      </src>
      <valueMergeType>Update</valueMergeType>
    </mapping>
  </attribute-mappings>
  <relationshipCriteria />
  <synchronizationCriteria>
    <conditions>
